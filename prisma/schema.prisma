generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Document {
  measure_uuid     String   @id @default(uuid())
  image_url        String
  measure_value    Int
  measure_datetime DateTime @default(now())
  has_confirmed    Boolean
}
